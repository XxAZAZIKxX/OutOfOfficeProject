{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "OutOfOffice_Server",
    "Audiences": [ "OutOfOffice" ],
    "TokenLifetime": 3600,
    "RefreshLifetime": 604800
  },
  "Database": {
    "Server": "localhost",
    "DatabaseName": "out_of_office_db"
  },
  "Redis": {
    "EndPoints": [ "127.0.0.1" ]
  }
}
